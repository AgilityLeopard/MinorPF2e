{
    "compilerOptions": {
        "target": "ES2020",
        "lib": ["DOM", "ES2020"],
        "types": ["@league-of-foundry-developers/foundry-vtt-types", "src/types", "handlebars"],
        "esModuleInterop": true,
        "moduleResolution": "node",
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "sourceMap": true,
        "strictNullChecks": true
    },
    "include": ["src"]
}
