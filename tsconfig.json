{
  "compilerOptions": {
    "target": "ES2021",
    "lib": [
      "DOM",
      "ES2021"
    ],
    "types": [
      "@league-of-foundry-developers/foundry-vtt-types",
      "handlebars"
    ],
    "typeRoots": [
      "src/types"
    ],
    "esModuleInterop": true,
    "moduleResolution": "node",
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "sourceMap": true,
    "strictNullChecks": true
  },
  "include": [
    "src"
  ]
}
